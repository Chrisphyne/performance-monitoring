omponentProps<{ name: string }> {}\r\n\r\nexport const DeviceDetail = (props: IDeviceDetailProps) => {\r\n  useEffect(() => {\r\n    props.getEntity(props.match.params.name);\r\n  }, []);\r\n\r\n  const { deviceEntity } = props;\r\n\r\n  return(\r\n    <Row>\r\n      <CCard>\r\n      <Col md=\"8\">\r\n        <CCardHeader>\r\n        <h2>\r\n          <b>{/[^/]*$/.exec(deviceEntity.name)[0]}</b>\r\n        </h2>\r\n        </CCardHeader>\r\n        <dl className=\"jh-entity-details\">\r\n          <dt>\r\n              <span id=\"name\">\r\n                {deviceEntity.name}\r\n              KeyguardDisabled\r\n            </span>\r\n          </dt>\r\n          <dd>{deviceEntity.userName}</dd>\r\n          <dt>\r\n            <span id=\"balance\">\r\n              StatusBarDisabled\r\n            </span>\r\n          </dt>\r\n          <dd>{deviceEntity.networkInfo.wifiMacAddress}</dd>\r\n          <dt>\r\n            Version\r\n          </dt>\r\n          <dd>{deviceEntity.appliedPolicyName}</dd>\r\n        </dl>\r\n        <Button tag={Link} to=\"/policies\" replace color=\"info\">\r\n          <FontAwesomeIcon icon=\"arrow-left\" />{' '}\r\n          <span className=\"d-none d-md-inline\">\r\n            <Translate contentKey=\"entity.action.back\">Back</Translate>\r\n          </span>\r\n        </Button>\r\n        &nbsp;\r\n        <Button tag={Link} to={`/policies/${/[^/]*$/.exec(deviceEntity.name)[0]}/edit`} replace color=\"primary\">\r\n          <FontAwesomeIcon icon=\"pencil-alt\" />{' '}\r\n          <span className=\"d-none d-md-inline\">\r\n            <Translate contentKey=\"entity.action.edit\">Edit</Translate>\r\n          </span>\r\n        </Button>\r\n      </Col>\r\n      </CCard>\r\n    </Row>\r\n  );\r\n\r\n}\r\n\r\n\r\nconst mapStateToProps = ({ device }: IRootState) => ({\r\n  deviceEntity: device.entity,\r\n});\r\n\r\nconst mapDispatchToProps = { getEntity };\r\n\r\ntype StateProps = ReturnType<typeof mapStateToProps>;\r\ntype DispatchProps = typeof mapDispatchToProps;\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(DeviceDetail);\r\n"]}]}